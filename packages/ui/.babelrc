{
  "plugins": [
    "babel-plugin-styled-components",
    ["module-resolver", {
      "alias": {
        "@components": "./src/components",
        "@utils": "./src/utils"
      }
    }]
  ],
  "presets": [
    [
      "env", {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        },
        "spec": true,
        "useBuiltIns": false
      }
    ],
    "react",
    "stage-0"
  ]
}
